<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/MainActivity_FrameLayout_FragmentContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/MainActivity_FrameLayout_FragmentContainer_marginBottom"/>

    <ImageView
        android:id="@+id/MainActivity_ImageView_ArrowToReturn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_arrow"
        android:layout_margin="@dimen/MainActivity_ImageView_ArrowToReturn_Margin"
        android:visibility="gone"/>

    <include
        layout="@layout/bottom_app_bar"/>

</androidx.coordinatorlayout.widget.CoordinatorLayout>